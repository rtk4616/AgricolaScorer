<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="GreenTheme" parent="android:Theme.Holo.Light">
        <item name="android:buttonStyle">@style/button</item>
        <item name="android:windowBackground">@drawable/grass</item>
        <item name="android:listViewStyle">@style/listView</item>
    	<item name="android:actionBarStyle">@style/actionBar</item>
    	<item name="android:expandableListViewStyle">@style/expandableListView</item>
    	<item name="android:checkboxStyle">@style/checkBox</item>
	    <item name="android:actionOverflowButtonStyle">@android:style/Widget.Holo.ActionButton.Overflow</item>
        <item name="android:spinnerItemStyle">@style/spinnerItem</item>
    </style>
            
    <style name="GreenTheme_NoActionBar" parent="@style/GreenTheme">
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="GreenTheme_Settings" parent="@style/GreenTheme">
    	<item name="android:textColor">#BBFFBB</item>
    </style>
    
	<style name="actionBar" parent="android:style/Widget.Holo.ActionBar">
	    <item name="android:titleTextStyle">@style/title</item>
	    <item name="android:displayOptions">showCustom</item>
	</style>
    
    <style name="grassBackground">
        <item name="android:background">@drawable/grass</item>
    </style>
    
    <style name="button" parent="android:style/Widget.Holo.Light.Button">
        <item name="android:textColor">#BBFFBB</item>
    </style>
            
    <style name="checkBox" parent="@android:style/Widget.Holo.Light.CompoundButton.CheckBox">
        <item name="android:textColor">#BBFFBB</item>
        <item name="android:textSize">20sp</item>
    </style>
    
    <style name="text">
        <item name="android:textColor">#BBFFBB</item>
    </style>
    
    <style name="biggerText" parent="@style/text">
        <item name="android:textSize">20sp</item>
        <item name="android:padding">5dp</item>
    </style>
    
    <style name="smallerText" parent="@style/biggerText">
        <item name="android:textSize">16sp</item>
    </style>
        
    <style name="title" parent="@android:style/TextAppearance.Large.Inverse">
        <item name="android:textColor">#DDFFDD</item>
        <item name="android:paddingBottom">10dp</item>
    </style>
    
    <style name="hint">
        <item name="android:textColor">#C0C0C0</item>
        <item name="android:padding">10dp</item>
    </style>
    
    <style name="historyGroupItem">
		<item name="android:paddingLeft">30dp</item>
    </style>
    
	<style name="historyDate">
		<item name="android:textColor">#DDFFDD</item>
   	    <item name="android:textSize">20sp</item>
    </style>
    
	<style name="scoreTabContainer">
        <item name="android:background">@drawable/primary_list_shape</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:paddingBottom">3dp</item>
    </style>
    	    
	<style name="scoreContainer">
        <item name="android:padding">2dp</item>
    </style>
        
    <style name="content">
        <item name="android:background">@drawable/primary_list_shape</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:padding">10dp</item>
    </style>
        
    <style name="black">
        <item name="android:background">@drawable/graph_shape</item>
        <item name="android:layout_margin">2dp</item>
    </style>
       
    <style name="listView" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="tabWidgetText" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textColor">#DDFFDD</item>
        <item name="android:background">@drawable/tab_indicator</item>
        <item name="android:padding">15dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    
    <style name="moreSpaceOnBottom">
        <item name="android:paddingBottom">10dp</item>
    </style>
    
    <style name="radioGroupStyle">
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="segmentedStyle">
        <item name="checkedTextColour">#FFFFFF</item>
        <item name="uncheckedTextColour">#FFFFFF</item>
    </style>
    
    <style name="fieldsStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#8C4D00</item> <!-- light -->
        <item name="checkedEndColour">#6E3700</item> <!-- to dark -->
		<item name="uncheckedStartColour">#8C4D00</item> <!-- light -->
		<item name="uncheckedEndColour">#303030</item> <!-- to really dark -->
	</style>
	        
    <style name="pasturesStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#1E7E27</item>
        <item name="checkedEndColour">#176114</item>
		<item name="uncheckedStartColour">#1E7E27</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
	        
    <style name="grainStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#F4E12D</item>
        <item name="checkedEndColour">#FAC020</item>
		<item name="uncheckedStartColour">#F4E12D</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="vegetableStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#FF960F</item>
        <item name="checkedEndColour">#EA7001</item>
		<item name="uncheckedStartColour">#FF960F</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="sheepStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#E7E1D5</item>
        <item name="checkedEndColour">#C4C1BA</item>
		<item name="uncheckedStartColour">#E7E1D5</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="wildBoarStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#8B8883</item>
        <item name="checkedEndColour">#686663</item>
		<item name="uncheckedStartColour">#8B8883</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="cattleStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#E57411</item>
        <item name="checkedEndColour">#A04F11</item>
		<item name="uncheckedStartColour">#E57411</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="familyMemberStyle" parent="@style/segmentedStyle">
        <item name="checkedStartColour">#3592DB</item>
        <item name="checkedEndColour">#136DB5</item>
		<item name="uncheckedStartColour">#3592DB</item>
		<item name="uncheckedEndColour">#303030</item>
	</style>
		        
    <style name="roomTypeStyle" parent="@style/segmentedStyle">
	</style>
	
    <style name="numberPicker">
        <item name="android:textColor">#DDFFDD</item>
    </style>
    
    <style name="expandableListView">
        <item name="android:groupIndicator">@drawable/expandable_list_view_indicator</item>
    </style>
    
    <style name="overflowMenu">
        <item name="android:popupBackground">#295D00</item>
    </style>

    <style name="spinnerItem" parent="@android:style/Widget.TextView.SpinnerItem">
        <item name="android:textColor">#BBFFBB</item>
        <item name="android:background">@drawable/spinner</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
	</style>

    <style name="totalScore">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:background">@drawable/total_score_footer</item>
    </style>

    <style name="unitScoreValue" parent="@style/text">
        <item name="android:ems">2</item>
        <item name="android:text">0</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:background">@drawable/unit_score_value</item>
        <item name="android:textColor">#ff313739</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="globalScoreValue" parent="@style/unitScoreValue">
        <item name="android:text">-1</item>
        <item name="android:textSize">20sp</item>
    </style>

    <!-- Finished table -->
    <style name="finishedTableAbstractCell">
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingLeft">2dp</item>
    </style>

    <style name="finishedTableHeaderRow" parent="@style/finishedTableAbstractCell">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="finishedTableRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="finishedTableRowFooter" parent="@style/finishedTableRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="finishedTableFirstCol" parent="@style/finishedTableAbstractCell">
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_weight">0</item>
        <item name="android:gravity">right</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="finishedTableHeaderCorner" parent="@style/finishedTableFirstCol">
        <item name="android:layout_weight">0</item>
    </style>

    <style name="finishedTableHeaderCol" parent="@style/finishedTableAbstractCell">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="finishedTableUnitScore" parent="@style/finishedTableAbstractCell">
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@drawable/table_body_border</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="finishedTableUnitScoreMax" parent="@style/finishedTableUnitScore">
        <item name="android:background">@drawable/table_body_border_max</item>
    </style>

    <style name="finishedTableUnitScoreMin" parent="@style/finishedTableUnitScore">
        <item name="android:background">@drawable/table_body_border_min</item>
    </style>

    <style name="finishedTableTotalScoreHeader" parent="@style/finishedTableFirstCol">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="finishedTableTotalScore" parent="@style/finishedTableAbstractCell">
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@drawable/table_body_border</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="finishedTableTotalScoreMax" parent="@finishedTableTotalScore">
        <item name="android:background">@drawable/table_body_border_max</item>
    </style>

    <style name="finishedTableTotalScoreMin" parent="@finishedTableTotalScore">
        <item name="android:background">@drawable/table_body_border_min</item>
    </style>

</resources>